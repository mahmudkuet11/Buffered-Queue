{
    "name": "mahmud/buffered-queue",
    "description": "A simple to use buffering queue as PHP package  (no dependencies), which flexibly buffers objects, strings, integers etc. until a maximum size is reached",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mahmud Ur Rahman",
            "email": "mahmudkuet11@gmail.com",
            "homepage": "https://mahmud.live"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.3"
    },
    "autoload": {
        "psr-4": {
            "Mahmud\\BufferedQueue\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mahmud\\BufferedQueue\\Tests\\": "tests"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "keywords": ["Buffered Queue"],
    "require-dev": {
        "phpunit/phpunit": "^7"
    }
}
